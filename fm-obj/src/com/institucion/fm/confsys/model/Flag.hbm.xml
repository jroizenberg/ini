<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC
  "-//hibernate/hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.institucion.fm.confsys.model">
	<class name="Flag" table="Flag">
		<id name="id" column="id">
			<generator class="sequence">
				<param name="sequence">Flag_id_seq</param>
			</generator>
		</id>
		<property name="reloadPermission" column="reload_permission" />
	</class>
</hibernate-mapping>